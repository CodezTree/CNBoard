<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.teammgh.cnboard">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="whateverString"
        android:icon="@drawable/logo_3"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:appComponentFactory">
        <activity android:name=".FoodDetail"
            android:parentActivityName=".LakeViewActivity"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".NoticeDetailActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".FoodActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".SplashActivity" >

        </activity>
        <activity android:name=".EnrolmentSubject"
            android:parentActivityName=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name=".DdayActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".DdaySettingActivity"
            android:parentActivityName=".DdayActivity"/>
        <activity android:name=".LakeViewActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".InputSubject" />

        <service
            android:name=".DdayService"
            android:enabled="true"
            android:exported="true"/>
    </application>

</manifest>